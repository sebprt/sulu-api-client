{
    "name": "sebprt/sulu-api-client",
    "description": "A PHP client library for the Sulu CMS API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "SÃ©bastien Parrat",
            "email": "sebastien.parrat@orange.fr"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.0",
        "guzzlehttp/psr7": "^2.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^2.0",
        "symfony/http-client": "^5.4|^6.0|^7.0",
        "symfony/http-foundation": "^5.4|^6.0|^7.0",
        "php-http/discovery": "^1.20",
        "psr/http-factory-implementation": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.5",
        "nyholm/psr7": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Sulu\\ApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sulu\\ApiClient\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
