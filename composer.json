{
    "name": "sebprt/sulu-api-client",
    "description": "A PHP client library for the Sulu CMS API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "SÃ©bastien Parrat <sebastien.parrat@orange.fr>"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.0",
        "guzzlehttp/psr7": "^2.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "symfony/http-client": "^5.4|^6.0",
        "symfony/http-foundation": "^5.4|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Sulu\\ApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sulu\\ApiClient\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
